{
  "name": "@raideno/convex-onboardings",
  "version": "0.0.1",
  "description": "A reusable Convex package for building onboardings.",
  "keywords": [
    "onboarding",
    "convex",
    "react"
  ],
  "files": [
    "dist",
    "README.md",
    "../../README.md"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./server": {
      "import": "./dist/server.js",
      "types": "./dist/server.d.ts"
    },
    "./schema": {
      "import": "./dist/schema.js",
      "types": "./dist/schema.d.ts"
    },
    "./client": {
      "import": "./dist/client.js",
      "types": "./dist/client.d.ts"
    }
  },
  "homepage": "https://github.com/raideno/convex-onboardings",
  "bugs": "https://github.com/raideno/convex-onboardings/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/raideno/convex-onboardings"
  },
  "license": "Apache-2.0",
  "author": "raideno",
  "scripts": {
    "clean": "rimraf dist",
    "build": "vite build",
    "dev": "vite build --watch"
  },
  "type": "module",
  "peerDependencies": {
    "convex": "^1.26.2",
    "react": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@convex-dev/auth": "^0.0.88",
    "@types/node": "^24.3.0",
    "@types/react": "^18.3.27",
    "@types/react-dom": "^18.3.7",
    "convex": "^1.0.2",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "rollup-plugin-visualizer": "^6.0.3",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.41.0",
    "vite": "^7.1.5",
    "vite-plugin-compile-time": "^0.4.6",
    "vite-plugin-dts": "^4.5.4",
    "vite-tsconfig-paths": "^5.1.4"
  }
}